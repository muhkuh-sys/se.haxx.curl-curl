set(@PROJECT_NAME@_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

ADD_LIBRARY(@PROJECT_NAME@ STATIC IMPORTED)
SET_TARGET_PROPERTIES(@PROJECT_NAME@
                      PROPERTIES IMPORTED_LIBNAME @LIB_NAME@
                      IMPORTED_LOCATION "@PACKAGE_LIBRARY_INSTALL_DIR@/@P_LIBRARY_NAME@"
                      INTERFACE_INCLUDE_DIRECTORIES "@PACKAGE_INCLUDE_INSTALL_DIR@"
                      INTERFACE_LINK_LIBRARIES "@P_LINK_LIBRARIES@"
                      INTERFACE_LINK_FLAGS "-L@PACKAGE_LIBRARY_INSTALL_DIR@"
                      INTERFACE_POSITION_INDEPENDENT_CODE ON
)
